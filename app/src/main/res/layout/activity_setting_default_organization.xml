<?xml version="1.0" encoding="utf-8"?>
<!--设置默认组织-->
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <com.jeewms.www.wms.ui.view.TitleTopOrdersView
        android:id="@+id/organ_title"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_width="0dp"
        android:layout_height="wrap_content"/>
    <android.support.constraint.Guideline
        android:id="@+id/guideline1"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.05"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>
    <android.support.constraint.Guideline
        android:id="@+id/guideline2"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.95"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>
    <RelativeLayout
        android:id="@+id/setting_warehouse"
        android:paddingTop="5dp"
        android:paddingBottom="5dp"
        app:layout_constraintTop_toBottomOf="@id/organ_title"
        app:layout_constraintLeft_toLeftOf="@id/guideline1"
        app:layout_constraintRight_toRightOf="@id/guideline2"
        android:layout_width="0dp"
        android:layout_height="wrap_content">
        <TextView
            android:id="@+id/warehouse_tv"
            android:padding="10dp"
            style="@style/mine_fragment"
            android:text="@string/setting_2"
            android:layout_centerVertical="true"
            android:layout_alignParentLeft="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
        <ImageView
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            android:src="@drawable/gray_right"
            android:layout_width="25dp"
            android:layout_height="25dp"/>
    </RelativeLayout>
    <View
        android:id="@+id/view1"
        app:layout_constraintLeft_toLeftOf="@id/guideline1"
        app:layout_constraintRight_toRightOf="@id/guideline2"
        app:layout_constraintTop_toBottomOf="@+id/setting_warehouse"
        android:background="@color/actions_background_light"
        android:layout_width="0dp"
        android:layout_height="1dp"/>
    <RelativeLayout
        android:paddingTop="5dp"
        android:paddingBottom="5dp"
        android:id="@+id/setting_deliver"
        app:layout_constraintTop_toBottomOf="@id/view1"
        app:layout_constraintLeft_toLeftOf="@id/guideline1"
        app:layout_constraintRight_toRightOf="@id/guideline2"
        android:layout_width="0dp"
        android:layout_height="wrap_content">
        <TextView
            android:id="@+id/deliver_tv"
            android:padding="10dp"
            style="@style/mine_fragment"
            android:text="@string/setting_3"
            android:layout_centerVertical="true"
            android:layout_alignParentLeft="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
        <ImageView
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            android:src="@drawable/gray_right"
            android:layout_width="25dp"
            android:layout_height="25dp"/>
    </RelativeLayout>

    <View
        android:id="@+id/view2"
        app:layout_constraintLeft_toLeftOf="@id/guideline1"
        app:layout_constraintRight_toRightOf="@id/guideline2"
        app:layout_constraintTop_toBottomOf="@+id/setting_deliver"
        android:background="@color/actions_background_light"
        android:layout_width="0dp"
        android:layout_height="1dp"/>
    <RelativeLayout
        android:paddingTop="5dp"
        android:paddingBottom="5dp"
        android:id="@+id/setting_type"
        app:layout_constraintTop_toBottomOf="@id/view2"
        app:layout_constraintLeft_toLeftOf="@id/guideline1"
        app:layout_constraintRight_toRightOf="@id/guideline2"
        android:layout_width="0dp"
        android:layout_height="wrap_content">
        <TextView
            android:id="@+id/type_tv"
            android:padding="10dp"
            style="@style/mine_fragment"
            android:text="@string/setting_3_1"
            android:layout_centerVertical="true"
            android:layout_alignParentLeft="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
        <ImageView
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            android:src="@drawable/gray_right"
            android:layout_width="25dp"
            android:layout_height="25dp"/>
    </RelativeLayout>

    <View
        android:id="@+id/view3"
        app:layout_constraintLeft_toLeftOf="@id/guideline1"
        app:layout_constraintRight_toRightOf="@id/guideline2"
        app:layout_constraintTop_toBottomOf="@+id/setting_type"
        android:background="@color/actions_background_light"
        android:layout_width="0dp"
        android:layout_height="1dp"/>
    <RelativeLayout
        android:paddingTop="5dp"
        android:paddingBottom="5dp"
        android:id="@+id/setting_Receiving_department"
        app:layout_constraintTop_toBottomOf="@id/view3"
        app:layout_constraintLeft_toLeftOf="@id/guideline1"
        app:layout_constraintRight_toRightOf="@id/guideline2"
        android:layout_width="0dp"
        android:layout_height="wrap_content">
        <TextView
            android:id="@+id/Receiving_department_tv"
            android:padding="10dp"
            style="@style/mine_fragment"
            android:text="@string/setting_3_0"
            android:layout_centerVertical="true"
            android:layout_alignParentLeft="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
        <ImageView
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            android:src="@drawable/gray_right"
            android:layout_width="25dp"
            android:layout_height="25dp"/>
    </RelativeLayout>

    <View
        android:id="@+id/view4"
        app:layout_constraintLeft_toLeftOf="@id/guideline1"
        app:layout_constraintRight_toRightOf="@id/guideline2"
        app:layout_constraintTop_toBottomOf="@+id/setting_Receiving_department"
        android:background="@color/actions_background_light"
        android:layout_width="0dp"
        android:layout_height="1dp"/>
    <RelativeLayout
        android:paddingTop="5dp"
        android:paddingBottom="5dp"
        android:id="@+id/setting_Receiving_organization"
        app:layout_constraintTop_toBottomOf="@id/view4"
        app:layout_constraintLeft_toLeftOf="@id/guideline1"
        app:layout_constraintRight_toRightOf="@id/guideline2"
        android:layout_width="0dp"
        android:layout_height="wrap_content">
        <TextView
            android:id="@+id/Receiving_organization_tv"
            android:padding="10dp"
            style="@style/mine_fragment"
            android:text="@string/setting_3_1"
            android:layout_centerVertical="true"
            android:layout_alignParentLeft="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
        <ImageView
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            android:src="@drawable/gray_right"
            android:layout_width="25dp"
            android:layout_height="25dp"/>
    </RelativeLayout>


    <View
        android:id="@+id/view5"
        app:layout_constraintLeft_toLeftOf="@id/guideline1"
        app:layout_constraintRight_toRightOf="@id/guideline2"
        app:layout_constraintTop_toBottomOf="@+id/setting_Receiving_organization"
        android:background="@color/actions_background_light"
        android:layout_width="0dp"
        android:layout_height="1dp"/>
    <RelativeLayout
        android:paddingTop="5dp"
        android:paddingBottom="5dp"
        android:id="@+id/setting_Demand_organization"
        app:layout_constraintTop_toBottomOf="@id/view5"
        app:layout_constraintLeft_toLeftOf="@id/guideline1"
        app:layout_constraintRight_toRightOf="@id/guideline2"
        android:layout_width="0dp"
        android:layout_height="wrap_content">
        <TextView
            android:id="@+id/Demand_organization_tv"
            android:padding="10dp"
            style="@style/mine_fragment"
            android:text="@string/setting_3_2"
            android:layout_centerVertical="true"
            android:layout_alignParentLeft="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
        <ImageView
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            android:src="@drawable/gray_right"
            android:layout_width="25dp"
            android:layout_height="25dp"/>
    </RelativeLayout>

    <View
        android:id="@+id/view6"
        app:layout_constraintLeft_toLeftOf="@id/guideline1"
        app:layout_constraintRight_toRightOf="@id/guideline2"
        app:layout_constraintTop_toBottomOf="@+id/setting_Demand_organization"
        android:background="@color/actions_background_light"
        android:layout_width="0dp"
        android:layout_height="1dp"/>
    <RelativeLayout
        android:paddingTop="5dp"
        android:paddingBottom="5dp"
        android:id="@+id/setting_Purchasing_organization"
        app:layout_constraintTop_toBottomOf="@id/view6"
        app:layout_constraintLeft_toLeftOf="@id/guideline1"
        app:layout_constraintRight_toRightOf="@id/guideline2"
        android:layout_width="0dp"
        android:layout_height="wrap_content">
        <TextView
            android:id="@+id/Purchasing_organization_tv"
            android:padding="10dp"
            style="@style/mine_fragment"
            android:text="@string/setting_3_3"
            android:layout_centerVertical="true"
            android:layout_alignParentLeft="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
        <ImageView
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            android:src="@drawable/gray_right"
            android:layout_width="25dp"
            android:layout_height="25dp"/>
    </RelativeLayout>
</android.support.constraint.ConstraintLayout>