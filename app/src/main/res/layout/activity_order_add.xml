                android:layout_height="5dp"
                android:background="@color/actions_background_light"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toBottomOf="@id/barrier2" />

            <TextView
                android:id="@+id/tv_order_details_tv4"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:lines="1"
                android:paddingTop="10dp"
                android:paddingBottom="10dp"
                app:layout_constrainedWidth="true"
                app:layout_constraintHorizontal_bias="0"
                app:layout_constraintLeft_toLeftOf="@id/guideline1"
                app:layout_constraintRight_toRightOf="@id/guideline2"
                app:layout_constraintTop_toBottomOf="@id/view2"
                app:layout_constraintWidth_default="wrap"
                tool:text="选择商" />

            <ImageView
                android:id="@+id/iv_order_details_iv2"
                android:layout_width="30dp"
                android:layout_height="0dp"
                android:src="@drawable/back_red"
                app:layout_constraintBottom_toBottomOf="@+id/tv_order_details_tv4"
                app:layout_constraintDimensionRatio="1:1"
                app:layout_constraintStart_toStartOf="@id/guideline2"
                app:layout_constraintTop_toTopOf="@+id/tv_order_details_tv4" />

            <ImageView
                android:id="@+id/iv_order_details_iv3"
                android:layout_width="30dp"
                android:layout_height="0dp"
                android:layout_marginLeft="30dp"
                android:src="@drawable/back_red"
                app:layout_constraintBottom_toBottomOf="@+id/tv_order_details_tv4"
                app:layout_constraintDimensionRatio="1:1"
