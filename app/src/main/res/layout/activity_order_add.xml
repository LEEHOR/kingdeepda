                android:gravity="center_vertical"
                android:hint="请输入/查询"
                android:maxLength="9"
                android:maxLines="1"
                android:text="大大萨达撒大萨达撒达撒大萨达撒是的撒大"
                app:layout_constraintBottom_toBottomOf="@+id/tv_order_details_tv1"
                app:layout_constraintHorizontal_chainStyle="spread_inside"
                app:layout_constraintLeft_toLeftOf="@id/guideline2"
                app:layout_constraintRight_toLeftOf="@+id/iv_order_details_iv1"
                app:layout_constraintTop_toTopOf="@+id/tv_order_details_tv1"
                app:layout_constraintWidth_default="spread" />

            <ImageView
                android:id="@+id/iv_order_details_iv1"
                android:layout_width="20dp"
                android:layout_height="0dp"
                android:contentDescription="TODO"
                android:src="@drawable/back_red"
                app:layout_constraintBottom_toBottomOf="@+id/et_order_details_et1"
                app:layout_constraintDimensionRatio="1:1"
                app:layout_constraintLeft_toRightOf="@+id/et_order_details_et1"
                app:layout_constraintRight_toRightOf="@id/guideline3"
                app:layout_constraintTop_toTopOf="@+id/et_order_details_et1" />

            <android.support.constraint.Barrier
                android:id="@+id/barrier1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:barrierDirection="bottom"
                app:constraint_referenced_ids="tv_order_details_tv1,et_order_details_et1"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent" />

            <TextView
